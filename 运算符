#include <iostream>
using namespace std;
int main()
{
	//加减乘除
	int a1 = 10;
	int b1 = 3;
	cout << a1 + b1 << endl; //13
	cout << a1 - b1 << endl; //7
	cout << a1 * b1 << endl; //30 
	cout << a1 / b1 << endl; //3
	system("pause");
	return 0;
}


#include <iostream>
using namespace std;

int main()
{
	//取模运算（求余数）
	int a1 = 10;
	int b1 = 3;
	cout << a1 % b1 << endl; //1
	system("pause");
	return 0;
}




